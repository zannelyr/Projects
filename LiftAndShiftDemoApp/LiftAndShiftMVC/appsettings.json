{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LocalConnection": "Server={SERVER_NAME};Database={DB_NAME};Trusted_Connection=True;",
    "AzureConnection": "Server=tcp:{SERVER_NAME}.database.windows.net,1433;Initial Catalog={DB_NAME};Persist Security Info=False;User ID={USER_ID};Password={PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  }
}
